<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Test</title>
    <script src="js/app.js"></script>
  </head>
  <body>
    <div class="card" style="width: 18rem">
      <% users.forEach(user => { %>
      <div class="card-body">
        <h2 class="card-title">Name: <%= user.name %></h2>
        <p class="card-text">Last Name: <%= user.lastName %></p>
        <p class="card-text">Is Admin: <%= user.admin ? 'Yes ðŸ˜Ž': 'No' %></p>
        <p class="card-subtitle mb-2 text-muted">Email: <%= user.email %></p>
        <button onclick="removeUser('<%= user._id %>')">Remove</button>
      </div>
      <% }) %>
    </div>
    <div id="containerUsers"></div>
  </body>
</html>
